<Project>

	<PropertyGroup>
		<CurrentTime>$([System.DateTime]::Now.ToString("yyyy-MM-dd-HH-mm-ss"))</CurrentTime>
		<CommitMessage>auto commit</CommitMessage>
	</PropertyGroup>



	<Import Project="README.md.template"/>


	<Target Name="ApplyTemplate">
		<CallTarget Targets="@(Generate_Template_File_Targets)"/>
	</Target>


	<Target Name="Push" AfterTargets="ApplyTemplate" Condition="Exists('$(TOKEN_AUTO_PUSH)')">
		<Exec Command='git config --global user.email "yucaizi1984@gmail.com"'/>
		<Exec Command='git config --global user.name "ZiYuCai_Automation"'/>
		<Exec Command='git add .'/>
		<Exec Command='git commit -m "$(CommitMessage)"'/>
		<Exec Command="git push --set-upstream https://$(TOKEN_AUTO_PUSH)@github.com/ZiYuCai1984/Rustle.fmLuWorks.Automation.DailyPush master"/>
	</Target>


</Project>    