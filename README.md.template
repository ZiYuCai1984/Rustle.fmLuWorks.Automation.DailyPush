<Project>
    <PropertyGroup>
        <README_md>
<![CDATA[
# Auto update

## Current time

$(CurrentTime)

]]>
        </README_md>
    </PropertyGroup>

    <Target Name="Generate_README_md">
        <WriteLinesToFile
            File="README.md"
            Lines="$(README_md)"
            Overwrite="true"
            Encoding="utf-8"/>
    </Target>

    <ItemGroup>
        <Generate_Template_File_Targets
            Include="Generate_README_md"/>
    </ItemGroup>

</Project>






